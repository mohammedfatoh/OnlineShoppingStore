                        <a class="dropdown-item " asp-area="Identity" asp-page="/Account/Login">Sign In</a>
                        <a class="dropdown-item" asp-area="Identity" asp-page="/Account/Register">Sign Up</a>
                    </div>
                </li>
            </ul>
        </div>
    </div>
</nav>
     
 <div id="wrapper">
